{
  "name": "research-vite-app",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "description": "Modern full-stack file management application with S3-compatible storage",
  "author": "Research Space Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/PRATS-gits/research-vite-app.git"
  },
  "scripts": {
    "dev": "vite",
    "dev:backend": "cd backend && npm run dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:backend\" --names \"frontend,backend\" --prefix-colors \"cyan,magenta\"",
    "build": "tsc -b && vite build",
    "build:backend": "cd backend && npm run build",
    "build:all": "npm run build && npm run build:backend",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:backend": "cd backend && npm run lint",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "test": "echo \"Frontend tests not yet implemented\"",
    "test:backend": "cd backend && npm test",
    "test:all": "npm test && npm run test:backend",
    "clean": "rm -rf dist node_modules",
    "clean:all": "npm run clean && cd backend && npm run clean",
    "install:all": "npm install && cd backend && npm install",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "db:studio": "cd backend && npx prisma studio",
    "db:migrate": "cd backend && npx prisma migrate dev",
    "db:generate": "cd backend && npx prisma generate",
    "postinstall": "cd backend && npm install"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@remixicon/react": "^4.6.0",
    "@tailwindcss/vite": "^4.1.14",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "jszip": "^3.10.1",
    "lucide-react": "^0.544.0",
    "next-themes": "^0.4.6",
    "react": "^19.2.0",
    "react-day-picker": "^9.11.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.3",
    "recharts": "^3.2.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.14",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/node": "^24.6.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^5.0.3",
    "concurrently": "^9.1.2",
    "eslint": "^9.37.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.23",
    "globals": "^16.4.0",
    "prettier": "^3.4.2",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.9"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "react",
    "typescript",
    "vite",
    "s3",
    "file-management",
    "cloud-storage",
    "tailwindcss",
    "shadcn-ui"
  ]
}
